package Aula04;

public class Caneta {
	
	public String modelo;
	private float ponta;
	
	public String getModelo() {
		return this.modelo;
	}
	public void setModelo(String m) {	
		this.modelo = m;
	}
	public float getPonta() {
		return this.ponta;
	}
	public void setPonta(float f) {
		this.ponta = f;
	}
}
